{
  "name": "connectmd",
  "private": true,
  "version": "0.0.0",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "3.3.0",
    "lerna": "^3.22.1",
    "prettier": "^2.1.1"
  },
  "scripts": {
    "version:new": "lerna version --conventional-commits",
    "dev:server": "",
    "dev:web": "",
    "dev": "concurrently \"yarn dev:server\" \"yarn dev:web\"",
    "build:server": ""
  },
  "packages": [
    "packages/*"
  ],
  "useWorkspaces": true,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
